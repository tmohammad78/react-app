	"homepage": "https://tmohammad78.github.io/react-app",
	"name": "custom-web",
	"version": "0.0.1",
	"description": "Food Delivery",
	"author": "",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "concurrently  \"cross-env NODE_ENV=development webpack-dev-server  --env.mode=dev\"",
		"server": "nodemon server/app",
		"publish": "node server/app",
		"dev:bundleanalyzer": "yarn dev --env.addons=bundleanalyzer",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist",
		"build": "npm run clean & cross-env NODE_ENV=production webpack -p --env.mode=prod --env.PUBLIC_URL=http://localhost:5000 --mode production",
		"clean": "rimraf dist",
		"lint": "eslint --ext .jsx --ext .js src",
		"test": "jest --watchAll",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"jest": {
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"collectCoverageFrom": [
			"src/**/*.js",
			"!src/index.js"
		],
		"coverageReporters": [
			"text"
		],
		"setupFilesAfterEnv": [
			"./src/jestsetup.js"
		]
	},
