{
  "name": "custom-web",
  "version": "0.0.1",
  "description": "Food Delivery",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server\" \"cross-env NODE_ENV=development webpack-dev-server --env.mode=dev\"",
    "server": "nodemon server/app",
    "publish": "node server/app",
    "dev:bundleanalyzer": "yarn dev --env.addons=bundleanalyzer",
    "build": "npm run clean & cross-env NODE_ENV=production webpack -p --env.mode=prod --mode production",
    "clean": "rimraf dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "test": "jest --watchAll",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "setupFilesAfterEnv": [
      "./src/jestsetup.js"
    ]
  },
