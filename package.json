{
  "name": "custom-web",
  "version": "0.0.1",
  "description": "Food Delivery",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently  \"cross-env NODE_ENV=development webpack-dev-server  --env.mode=dev\"",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node dist/bundle.js \" ",
    "dev:build-server": "webpack --config webpack.config.js --watch --env.mode=prod",
    "dev:build-client": "webpack --config webpack.config.js --watch --env.mode=dev",
    "build": "npm run clean & cross-env NODE_ENV=production webpack -p --env.mode=prod --mode production",
    "clean": "rimraf dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "test": "jest --watchAll",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "setupFilesAfterEnv": [
      "./src/jestsetup.js"
    ]
  },
