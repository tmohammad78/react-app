{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "component/*": ["src/component/*"],
      "services/*": ["src/services/*"],
      "pages/*": ["src/pages/*"],
      "helper/*": ["src/helper/*"]
    },
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "tags": ["styled", "css", "sty"]
      }
    ]
  }
}
